<thlogic>
    <!--Fragments-->
    <attr sel="#appNavBar"
          th:replace="~{fragments/navbar :: navBarFragment}"/>
    <attr sel="#appFlashMessages"
          th:replace="~{fragments/flashes :: flashMessagesFragment}"/>

    <!--CSS-->
    <attr sel="#pageIcon1"
          th:href="@{/img/tab_icon.png}"/>
    <attr sel="#pageIcon2"
          th:href="@{/img/tab_icon.png}"/>
    <attr sel="#bootstrapCss"
          th:href="@{/css/bootstrap-4.1.3-dist/bootstrap.min.css}"/>
    <attr sel="#resetCss"
          th:href="@{/css/reset.css}"/>
    <attr sel="#applicationCss"
          th:href="@{/css/application.css}"/>
    <attr sel="#koGridCss"
          th:href="@{/libs/KoGrid-master/KoGrid.css}"/>


    <!--Variables-->
    <attr sel="#appTitle"
          th:text="#{application.name}"/>


    <!--JS-->
    <attr sel="#bootstrapJs"
          th:src="@{/js/bootstrap-4.1.3-dist/bootstrap.js}"/>
    <attr sel="#bootstrapPopperJs"
          th:src="@{/js/popper.min.js}"/>
    <attr sel="#bootstrapJQueryJs"
          th:src="@{/js/jquery-3.3.1.min.js}"/>
    <attr sel="#applicationJs"
          th:src="@{/js/application.js}"/>
    <attr sel="#fontAwesomeJs"
          th:src="@{/plug-in/fontawesome/js/all.js}"/>
    <attr sel="#knockoutJs"
          th:src="@{/libs/knockout-3.4.0.js}"/>
    <attr sel="#koGridJs"
          th:src="@{/libs/KoGrid-master/koGrid-2.1.1.js}"/>


</thlogic>